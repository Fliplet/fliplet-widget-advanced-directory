<ul>{{#each this}}
  {{{alphabet_divider}}}
  <li class="linked data-linked" data-type="entry" data-index="{{@index}}">
    <!-- <span class="icon fa fa-star"></span> -->
    <div class="list-desc">
      <p class="list-title">{{plaintext "Title" this}}</p>
      <!-- <p class="survey-notification"><span class="survey-warning"><i class="fa fa-exclamation-circle"></i> Please complete survey</span></p> -->
      <p class="time-place">{{moment "Session start" "HH:mm" this}} &ndash; {{moment "Session end" "HH:mm" this}} &#183; <span>{{Location}}</span></p>
    </div>
    <div class="list-tags">
      {{tag_filters "Tags" this}}
    </div>
  </li>
{{/each}}</ul>
